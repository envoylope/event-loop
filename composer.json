{
    "name": "envoylope/event-loop",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Envoylope\\EventLoop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envoylope\\EventLoop\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "asmblah/php-amqp-compat": "dev-event-loop-heartbeats",
        "react/event-loop": "^1.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2",
        "tasque/tasque": "^0.1",
        "tasque/event-loop": "^0.1"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
